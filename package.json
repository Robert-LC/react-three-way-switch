{
  "name": "@robert-lc/react-three-way-switch",
  "version": "0.0.1",
  "description": "react three option toggle switch",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm-run-all --parallel start watch && echo 'Server started at http://localhost:5000' && open http://localhost:5000",
    "format": "prettier --write '**/*.{ts,tsx,json}",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "serve -s public -p 5000",
    "watch": "rollup -c -w"
  },
  "keywords": [
    "react",
    "triple toggle",
    "three way switch",
    "triple toggle switch"
  ],
  "author": "Robert-LC (https://github.com/Robert-LC)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "npm": "^10.8.1",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "serve": "^14.2.3",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/robert-lc"
  }
}
